{
  "node_name": "consul_daemon",
  "data_dir": "/var/consul",
  "server": false,
  "start_join": [
    "configuration"
  ],
  "retry_join": [
    "configuration"
  ],
  "watches": [
    {
      "type": "key",
      "key": "data_source/postgresql_daemon/host",
      "handler_type": "script",
      "args": ["sh", "-c", "/etc/consul.d/watch_handler.sh"]
    }
  ]
}